9 - VETSI KOD
________________________________________________________________

scope 'Hlavni telo programu
Dim a As Integer
DIM vysl AS INTEGER
PrinT !"Zadejte cislo pro vypocet faktorialu";
InpuT A
If a < 0 THEN
print !"\nFaktorial nelze spocitat\n";
ELSE
Vysl = 1
Do WHile A > 0
VYSL = vysl * a
a = A - 1
LooP
Print !"\nVysledek je:" ; vYsl ; !"\n";
end IF
END SCOPE


SCOPE EOL 
DIM ID AS INTEGER EOL 
DIM ID AS INTEGER EOL 
PRINT STRING_V SEMICOLON EOL 
INPUT ID EOL 
IF ID LESS INT_V THEN EOL 
PRINT STRING_V SEMICOLON EOL 
ELSE EOL 
ID ASSIGN INT_V EOL 
DO WHILE ID GREAT INT_V EOL 
ID ASSIGN ID MUL ID EOL 
ID ASSIGN ID SUB INT_V EOL 
LOOP EOL 
PRINT STRING_V SEMICOLON ID SEMICOLON STRING_V SEMICOLON EOL 
END IF EOL 
END SCOPE EOL 
EOF
xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

10 - velky kod
________________________________________________________________

Declare Function factorial (n As Integer) As Integer
Function factorial (n As Integer) As Integer
Dim temp_result As Integer
Dim decremented_n As Integer
Dim result As Integer
If n < 2 Then
result = 1
Else
decremented_n = n - 1
temp_result = factorial(decremented_n)
result = n * temp_result
End If
Return result
End Function
Scope 'Hlavni telo programu
Dim a As Integer
Dim vysl As Integer
Print !"Zadejte cislo pro vypocet faktorialu";
Input a
If a < 0 Then
Print !"\nFaktorial nelze spocitat\n";
Else
vysl = factorial(a)
Print !"\nVysledek je:" ; vysl ; !"\n";
End If
End Scope


DECLARE FUNCTION ID BRACKET_L ID AS INTEGER BRACKET_R AS INTEGER EOL 
FUNCTION ID BRACKET_L ID AS INTEGER BRACKET_R AS INTEGER EOL 
DIM ID AS INTEGER EOL 
DIM ID AS INTEGER EOL 
DIM ID AS INTEGER EOL 
IF ID LESS INT_V THEN EOL 
ID ASSIGN INT_V EOL 
ELSE EOL 
ID ASSIGN ID SUB INT_V EOL 
ID ASSIGN ID BRACKET_L ID BRACKET_R EOL 
ID ASSIGN ID MUL ID EOL 
END IF EOL 
RETURN ID EOL 
END FUNCTION EOL 
SCOPE EOL 
DIM ID AS INTEGER EOL 
DIM ID AS INTEGER EOL 
PRINT STRING_V SEMICOLON EOL 
INPUT ID EOL 
IF ID LESS INT_V THEN EOL 
PRINT STRING_V SEMICOLON EOL 
ELSE EOL 
ID ASSIGN ID BRACKET_L ID BRACKET_R EOL 
PRINT STRING_V SEMICOLON ID SEMICOLON STRING_V SEMICOLON EOL 
END IF EOL 
END SCOPE EOF

8 - KOUSEK KODU
________________________________________________________________

Dim x as Integer = 10
DIM y as Integer = 25
DIM z as Integer
z = x + y
print "Sum of x + y = "; z


DIM ID AS INTEGER ASSIGN INT_V EOL 
DIM ID AS INTEGER ASSIGN INT_V EOL 
DIM ID AS INTEGER EOL 
ID ASSIGN ID ADD ID EOL 
PRINT SCAN_ERR ID ID ADD ID ASSIGN SCAN_ERR ID EOL 
EOF
xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

9 - VETSI KOD
________________________________________________________________

scope 'Hlavni telo programu
	Dim a As Integer
	DIM vysl AS INTEGER
	PrinT !"Zadejte cislo pro vypocet faktorialu";
	InpuT A
	If a < 0 THEN
		print !"\nFaktorial nelze spocitat\n";
	ELSE
		Vysl = 1
		Do WHile A > 0
			VYSL = vysl * a
			a = A - 1
		LooP
		Print !"\nVysledek je:" ; vYsl ; !"\n";
	end IF
END SCOPE


SCOPE EOL 
DIM ID AS INTEGER EOL 
SCAN_ERR ID AS INTEGER EOL 
SCAN_ERR STRING_V EOF
xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

ALL DONE

' Condition with implicit conversion
scope
	if ((8*(2.0/9)+112.3 - 13.8) > (8*(2.0/9)+112.3 + 13.8)) then
		print 42;
	else
		print 58;
	end if
end scope



' chr builtin function
scope
	dim res as string
	dim tmp as integer
	tmp = 0 - 10

	res = chr(119)
	print res;
	res = chr(40)
	print res;
	res = chr(41.25)
	print res;
	res = chr(1)
	print res;
	
end scope



